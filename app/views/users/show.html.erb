<h1>Welcome <%= @user.name %>! </h1>

<%= link_to "Let's make a new event!", new_event_path  %>

<%# <%= binding.pry %1> %>
<% @user.events.each do |event| %>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
<br>
    <%= event.title %>
<br>
    <%= event.date %>
<br>
    <%= event.time %>
<br>
    <%= event.location %>
<br>
    <%= link_to "Edit", edit_event_path %>
    <%= link_to "Delete", event_path , method: 'delete' %>
  </div>
</div>
<% end %>
